{
    "name":              "jewsroch/wordpress-bootstrap",
    "type":              "composer-installer",
    "description":       "Wordpress Bootstrap",
    "keywords":          ["wordpress", "bootstrap"],
    "homepage":          "https://github.com/jewsroch/wordpress-bootstrap",
    "license":           "BSD-3-Clause",
    "authors":           [
        {
            "name":     "Andrew Vaughan",
            "email":    "andrew@deprecated.co",
            "homepage": "http://deprecated.co",
            "role":     "Developer"
        }
    ],
    "support":           {
        "source": "https://github.com/jewsroch/wordpress-bootstrap",
        "issues": "https://github.com/jewsroch/wordpress-bootstrap/issues",
        "wiki":   "https://github.com/jewsroch/wordpress-bootstrap/wiki"
    },
    "minimum-stability": "stable",
    "require":           {
        "php": ">=5.3.0"
    },
    "require-dev":       {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload":          {
        "psr-0": {
            "DeprecatedCo\\WPBootstrap": "src/"
        }
    },
    "extra":             {
        "class": [
            "DeprecatedCo\\WPBootstrap\\Composer\\Installer\\PluginInstaller",
            "DeprecatedCo\\WPBootstrap\\Composer\\Installer\\ThemeInstaller"
        ]
    }
}
